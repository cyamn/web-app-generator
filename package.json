    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.1",
