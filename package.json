{
  "name": "web-app-gen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:build": "cd database && npm run build && cd ..",
    "db:start": "cd database && npm run start && cd ..",
    "db": "npm run db:build && npm run db:start",
    "dev": "concurrently --kill-others-on-fail  -p \"{name} {time}|\" -t \"HH:mm:ss\" --names \"DATA,APP\" -c \"yellow,blue\" \"npm run db\" \"next dev\"",
    "start": "next start",
    "lint:types": "tsc --noEmit",
    "lint:conventions": "eslint src && prettier --check .",
    "lint:duplicates": "jscpd",
    "lint": "npm run lint:types && npm run lint:conventions && npm run lint:duplicates",
    "format": "eslint src --fix && prettier --write .",
    "postinstall": "prisma generate",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "copy-env": "cp .env.example .env",
    "bootstrap": "npm run copy-env && npm install && next build && concurrently --kill-others-on-fail  -p \"{name} {time}|\" -t \"HH:mm:ss\" --names \"DATA,APP\" -c \"yellow,blue\" \"npm run db\" \"next start\""
  },
  "dependencies": {
